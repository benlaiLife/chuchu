(function(){var app_env=getCookie('client_type');if(app_env=="ios"){initIOS()}function getCookie(name){var arr,reg=new RegExp("(^| )"+name+"=([^;]*)(;|$)");if(arr=document.cookie.match(reg))return arr[2];else return null}function initIOS(){var o={};var d='://com.culiu.JiuKuaiJiu';var s='####';function go(url){window.location.href=url}o.shareToWeixin=function(title,imgUrl,url,desc,type){go('sharetoweixin'+d+s+title+s+imgUrl+s+url+s+desc+s+type)};o.shareToWeixinFriends=function(title,imgUrl,url,desc,type){go('sharetoweixinfriends'+d+s+title+s+imgUrl+s+url+s+desc+s+type)};o.shareToQQFriends=function(title,imgUrl,url,desc,type){go('sharetoqqfriends'+d+s+title+s+imgUrl+s+url+s+desc+s+type)};o.shareToQQZone=function(title,imgUrl,url,desc,type){go('sharetoqqzone'+d+s+title+s+imgUrl+s+url+s+desc+s+type)};o.webLog=function(url){go('weblog'+d+s+url)};o.backgroundOpenUrl=function(url){go('backgroundopenurl'+d+s+url)};o.clearCookies=function(){go('clearcookies'+d)};o.setCookie=function(key,value,expire,domain,path){go('setcookie'+d+s+key+s+value+s+expire+s+domain+s+path)};o.getCookie=function(key,domain){go('getcookie'+d+s+key+s+domain)};o.pay=function(platform,orderInfo){go('pay'+d+s+platform+s+orderInfo)};o.syncLogin=function(token){go('synclogin'+d+s+token)};o.getClientInfo=function(){go('getclientInfo'+d)};o.callTemplate=function(template,query){go('calltemplate'+d+s+template+s+query)};o.resize=function(height){go('resize'+d+s+height)};o.shopLogin=function(){go('shoplogin'+d)};o.onlineService=function(paramsString){go('onlineservice'+d+s+paramsString)};o.localNotification_v2=function(paramsString){go('localnotification'+d+s+paramsString)};o.phoneService=function(paramsString){go('phoneservice'+d+s+paramsString)};o.HandleWapPayResult=function(paramsString){go('handlewappayresult'+d+s+paramsString)};o.shareConfig=function(paramsString){go('shareconfig'+d+s+paramsString)};o.mutiShareConfig=function(paramsString){go('mutiShareConfig'+d+s+paramsString)};o.mutiShareToWeChat=funct